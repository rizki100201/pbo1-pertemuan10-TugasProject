package toko;

public class class1 {
    //atribut
    public String pulpen;
    public String penggaris;
    public String penghapus;
    public String pensil;
    public String tipx;
    public String bukuTulis;
    public String gunting;
    public String tempatPensil;

    //method
    public void tampil() {
        System.out.println("Selamat Datang di Di Toko Saya");
    }

    public void plp() {
        System.out.println("pulpen " + pulpen);
    }

    public void pgr() {
        System.out.println("penggaris " + penggaris);
    }

    public void ph() {
        System.out.println("penghapus " + penghapus);
    }

    public void psl() {
        System.out.println("pensil " + pensil);
    }

    public void tp() {
        System.out.println("tipx " + tipx);
    }

    public void bt() {
        System.out.println("buku tulis " + bukuTulis);
    }

    public void g() {
        System.out.println("gunting " + gunting);
    }

    public void tpensil() {
        System.out.println("tempat pensil " + tempatPensil);
    }

    //constructor

    public class1() {
    }

    public class1(String pulpen, String penggaris, String penghapus, String pensil, String tipx, String bukuTulis, String gunting, String tempatPensil) {
        this.pulpen = pulpen;
        this.penggaris = penggaris;
        this.penghapus = penghapus;
        this.pensil = pensil;
        this.tipx = tipx;
        this.bukuTulis = bukuTulis;
        this.gunting = gunting;
        this.tempatPensil = tempatPensil;
    }

    public class1(String pulpen, String penggaris, String penghapus, String pensil) {
        this.pulpen = pulpen;
        this.penggaris = penggaris;
        this.penghapus = penghapus;
        this.pensil = pensil;
    }


}
